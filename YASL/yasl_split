!!
#
2 !=
? ("Too few argument\n" print 1 exit)
[#]
1
!=
? ("Invalid separator\n" print 1 exit)
=:1 //separator
!
=:2 //string
0:3 //incrementer
[#]:4 //string size
0:5 // \n not print
4:=
3:=
>
@ (
	3:=
	]#[
	=:6 //current char
	1:=
	==
	5:=
	&&
	? ("\n" print 0:5) : (
		6:=
		1:=
		!=
		? (1:5 6:= print 6:!)
	)
	2:=
	1:3
	3:+:3
	4:=
	3:=
	>
